services:
  catalog-api:
    build:
      context: D:\learning\CSharpLearning Courses\Microservices .net\Project\eshop-services
      dockerfile: Services/Catalog/Catalog.API/dockerfile
    depends_on:
      sqldata:
        condition: service_started
    environment:
      PATH_BASE: /catalog-apitest
      REGISTRY: eshopservice
    image: eshopservice/catalog.api-linux-latest
    ports:
    - published: 5101
      target: 80
    - published: 9101
      target: 81
  sqldata:
    image: mcr.microsoft.com/mssql/server:2019-latest
version: '3.4'